# include <bits/stdc++.h>
using namespace std;

int main()
{	int a,b;
	string n,s;
	vector<pair<string,string>> v;
	vector<pair<string,string>> v1;
	cin>>a>>b;
	for (int i = 0; i < a; ++i)
	{cin>>s>>n;
	    n.push_back(';');
		v.push_back({s,n});
	}
	for (int i = 0; i < b; ++i)
	{
		cin>>s>>n;
		v1.push_back({s,n});
	}
	for(auto it:v1){
		for(auto it1:v){
			if(it.second==it1.second)
			cout<<it.first<<" "<<it.second<<" "<<"#"<<it1.first<<'\n';

}
}
return 0;
}
